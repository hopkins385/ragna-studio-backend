{
  "name": "ragna-studio-backend",
  "version": "0.33.11",
  "description": "Ragna Studio Backend",
  "author": "Sven Stadhouders",
  "private": true,
  "license": "other",
  "scripts": {
    "build": "nest build",
    "build:seeds": "tsc -p prisma/tsconfig.prisma.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "types": "tsc --noEmit",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/e2e/jest-e2e.json",
    "migrate": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset --force && prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "migrate:applied": "prisma migrate resolve --applied 20250104182632_init",
    "seed:init": "npx ts-node --transpile-only prisma/seed/seed.ts",
    "seed:users": "npx ts-node --transpile-only prisma/seed/default-users.seed.ts",
    "seed:templates": "npx ts-node --transpile-only prisma/seed/assistant-templates.seed.ts",
    "build:ncc": "ncc build dist/main.js -o dist/ncc",
    "start:ncc": "node dist/ncc/index.js",
    "build:byte": "bytenode -c ./dist/ncc/index.js",
    "start:byte": "bytenode ./dist/ncc/index.jsc"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.18",
    "@ai-sdk/mistral": "^1.2.8",
    "@ai-sdk/openai": "^1.3.22",
    "@aws-sdk/client-s3": "^3.749.0",
    "@aws-sdk/lib-storage": "^3.749.0",
    "@keyv/redis": "^4.4.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.1.5",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.6.0",
    "@slack/socket-mode": "^2.0.4",
    "@slack/web-api": "^7.9.2",
    "ai": "^4.3.16",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.49.2",
    "cacheable": "^1.9.0",
    "csv-parse": "^5.6.0",
    "fast-json-stringify": "^6.0.1",
    "google-auth-library": "^9.15.1",
    "googleapis": "^149.0.0",
    "helmet": "^8.1.0",
    "keyv": "^5.3.3",
    "liquidjs": "^10.21.1",
    "ms": "^2.1.3",
    "nestjs-prisma": "^0.25.0",
    "nestjs-zod": "^4.3.1",
    "node-xlsx": "^0.24.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-extension-cuid2": "^1.1.1",
    "prisma-extension-pagination": "^0.7.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "serpapi": "^2.1.0",
    "sharp": "^0.34.2",
    "stripe": "^17.7.0",
    "tiktoken": "^1.0.21",
    "turndown": "^7.2.0",
    "zod": "^3.25.46"
  },
  "overrides": {
    "prisma-extension-cuid2": {
      "@prisma/client": "^6.1.0"
    },
    "nestjs-zod": {
      "@nestjs/common": "^11.0.6",
      "@nestjs/core": "^11.0.6",
      "@nestjs/swagger": "^11.0.3"
    }
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@faker-js/faker": "^9.8.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/ms": "^2.1.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.29",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "prisma-json-types-generator": "^3.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed/seed.ts"
  }
}
