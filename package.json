{
  "name": "ragna-studio-backend",
  "version": "0.0.1",
  "description": "Ragna Studio Backend",
  "author": "Sven Stadhouders",
  "private": true,
  "license": "other",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "prisma migrate dev",
    "migrate:fresh": "prisma migrate reset --force && prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "migrate:applied": "prisma migrate resolve --applied 20250104182632_init",
    "seed:init": "npx ts-node --transpile-only prisma/seed/seed.ts",
    "seed:users": "npx ts-node --transpile-only prisma/seed/default-users.seed.ts",
    "seed:templates": "npx ts-node --transpile-only prisma/seed/assistant-templates.seed.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.6",
    "@ai-sdk/google": "^1.0.12",
    "@ai-sdk/mistral": "^1.0.6",
    "@ai-sdk/openai": "^1.0.13",
    "@aws-sdk/client-s3": "^3.693.0",
    "@nestjs/bullmq": "^10.2.3",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^6.2.1",
    "ai": "^4.0.26",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.34.6",
    "csv-parse": "^5.6.0",
    "fast-json-stringify": "^6.0.0",
    "google-auth-library": "^9.15.0",
    "googleapis": "^144.0.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "liquidjs": "^10.19.1",
    "nestjs-prisma": "0.24.0-dev.0",
    "nestjs-zod": "^4.2.0",
    "node-xlsx": "^0.24.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-extension-cuid2": "^1.1.1",
    "prisma-extension-pagination": "^0.7.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "serpapi": "^2.1.0",
    "sharp": "^0.33.5",
    "stripe": "^17.3.1",
    "tiktoken": "^1.0.18",
    "zod": "^3.24.1"
  },
  "overrides": {
    "prisma-extension-cuid2": {
      "@prisma/client": "^6.1.0"
    }
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.1.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.13",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.8.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^6.2.1",
    "prisma-json-types-generator": "^3.2.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}